h := FRGroup("X=<X,X,t^-2*Y,t^-2*Y>(1,3)(2,4)",
  "Y=<t,t,t*Y/X,t*Y/X>(1,3)(2,4)",
  "t=<,t,,t>(1,2)(3,4)":IsFRMealyElement);

