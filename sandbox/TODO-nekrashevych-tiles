h := FRGroup("P=<Q,Q,,>","Q=<R,,R,>","R=<,S,S,>(1,2)(3,4)",
  "S=<P,P*S*U,,U^-1*S^-1>(1,3)(2,4)","T=<U,,,U>","U=<R^-1,,P^-1*Q^-1*T*Q*P,R*T>(1,4)(2,3)":IsFRMealyElement);

